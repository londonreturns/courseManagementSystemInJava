package utility;

import java.awt.Color;

import javax.swing.JLabel;

public class MenuLabel extends JLabel {
	
	MenuLabel(Color color){
		super();
		this.setBackground(color);
		this.setOpaque(true);
	}
	
	MenuLabel(String string){
		super(string);
		this.setBackground(Color.white);
		this.setOpaque(true);
	}
}
