package courseManagementSystem;

import java.awt.Cursor;
import javax.swing.JButton;

public class StandardButton extends JButton{
	StandardButton(){
		setCursor(new Cursor(Cursor.HAND_CURSOR));
	}
	
	StandardButton(String buttonName){
		super(buttonName);
		setCursor(new Cursor(Cursor.HAND_CURSOR));
	}
}