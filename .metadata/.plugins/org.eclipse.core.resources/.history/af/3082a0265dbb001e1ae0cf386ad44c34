package courseManagementSystem;

import java.awt.Color;

public class Main {
	static LoginRegisterFrame loginRegisterFrame = new LoginRegisterFrame(200, 200, 1000, 750, Color.black);

	public static void main(String[] args) {
		
	}

	static void loginPanelDiplay() {
		if (loginRegisterFrame.loginInit == false) {
			loginRegisterFrame.loginInit = true;
			loginRegisterFrame.setLoginElements();
			loginRegisterFrame.setVisible(true);
		}
		
	}
	
	static void registerPanelDisplay() {
		
	}
}
